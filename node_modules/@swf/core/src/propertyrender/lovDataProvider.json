{
    "dataProviders": {
        "tcDataprovider": {
            "initializeAction": "loadTCLOVDataFirstPage",
            "nextAction": "loadTCLOVDataNextPage",
            "validateAction": "validateTCLOV",
            "response": "{{data.lovDataInfo.lovEntries}}",
            "moreValuesExist": "{{data.lovDataInfo.moreValuesExist}}",
            "lovUsage": "{{data.lovDataInfo.lovUsage}}"
        },
        "listboxProvider": {
            "initializeAction": "convertList",
            "response": "{{data.listData.lovEntries}}"
        }
    },
    "actions": {
        "convertList": {
            "actionType": "JSFunctionAsync",
            "method": "getListLOVValues",
            "inputData": {
                "list": "{{parameters.prop.list}}"
            },
            "outputData": {
                "listData": null
            },
            "deps": "js/lovService"
        },
        "loadTCLOVDataFirstPage": {
            "actionType": "JSFunctionAsync",
            "method": "getInitialLOVValues",
            "inputData": {
                "viewModel": "{{data}}",
                "dataProviderName": "tcDataprovider"
            },
            "outputData": {
                "lovDataInfo": "{{function:processLOVEntries}}"
            },
            "deps": "js/lovService"
        },
        "loadTCLOVDataNextPage": {
            "actionType": "JSFunctionAsync",
            "method": "getNextValues",
            "inputData": {
                "viewModel": "{{data}}",
                "dataProviderName": "tcDataprovider"
            },
            "outputData": {
                "lovDataInfo": "{{function:processLOVEntries}}"
            },
            "deps": "js/lovService"
        },
        "validateTCLOV": {
            "actionType": "JSFunctionAsync",
            "method": "validateLOVValueSelections",
            "inputData": {
                "lovEntries": "request.selected",
                "viewProp": "request.vmProp",
                "operationName": "request.vmo.operationName",
                "vmo": "request.vmo",
                "owningObjUid": "request.vmProp.owningObjUid",
                "dataProviderName": "tcDataprovider"
            },
            "deps": "js/lovService"
        }
    },
    "functions": {
        "processLOVEntries": {
            "functionName": "processLOVEntries"
        },
        "deps": "js/lovService"
    }
}

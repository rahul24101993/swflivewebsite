{
    "schemaVersion": "1.0.0",
    "data": {},
    "actions": {
        "render": {
            "method": "commonAvatarRenderFunction",
            "deps": "js/CommonAvatarService"
        },
        "userAvatarAction": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [ {
                    "name": "awsidenav.openClose",
                    "eventData": {
                        "id": "globalNavigationSideNav",
                        "commandId": "avatarCommandId",
                        "includeView": "AwGlobalAvatar",
                        "keepOthersOpen": true,
                        "config": {
                            "width": "{{ctx.awSidenavConfig && ctx.awSidenavConfig.avatarPanel && ctx.awSidenavConfig.avatarPanel.width ? ctx.awSidenavConfig.avatarPanel.width : 'STANDARD' }}",
                            "height": "{{ctx.awSidenavConfig && ctx.awSidenavConfig.avatarPanel && ctx.awSidenavConfig.avatarPanel.height ? ctx.awSidenavConfig.avatarPanel.height : 'FULL' }}"
                        }
                    },
                    "excludeLocalDataCtx": true
                } ]
            }
        }
    },
    "ctx": {
        "user.hasThumbnail": null,
        "user.typeIconURL": null,
        "user.thumbnailURL": null,
        "user.cellHeader1": null
    },
    "onEvent": [],
    "i18n": {},
    "messages": {},
    "conditions": {},
    "lifecycleHooks": {
        "render": "render"
    }
}

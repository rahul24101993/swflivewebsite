{
    "schemaVersion": "1.0.0",
    "data": {
        "selectionData": {
            "initialValues": {},
            "meta": {}
        }
    },
    "ports": {
        "swaSelectionInput": {
            "syncObject": {}
        }
    },
    "actions": {
        "handleSelectionChange": {
            "actionType": "JSFunction",
            "method": "handleSelectionChange",
            "inputData": {
                "localSelectionData": "{{data.selectionData}}",
                "parentSelectionData": "{{props.selectionData}}"
            },
            "deps": "js/AwSecondaryWorkareaService"
        },
        "resetSelectionData": {
            "actionType": "JSFunction",
            "method": "resetSelectionData",
            "inputData": {
                "localSelectionData": "{{data.selectionData}}",
                "parentSelectionData": "{{props.selectionData}}"
            },
            "deps": "js/AwSecondaryWorkareaService"
        }
    },
    "lifecycleHooks": {
        "onUnmount": "resetSelectionData",
        "onUpdate": {
            "action": "handleSelectionChange",
            "observers": [ "viewModel.atomicData.selectionData" ]
        }
    },
    "props": {
        "sub-panel-context": {
            "type": "object"
        },
        "base-selection": {
            "type": "object"
        },
        "selection-data": {
            "type": "object"
        }
    }
}

{
    "schemaVersion": "1.0.0",
    "data": {
        "sideNavLeftData": {
            "slide": "FLOAT",
            "direction": "RIGHT_TO_LEFT",
            "animation": false,
            "width": "STANDARD",
            "height": "FULL"
        },
        "sideNavRightData": {
            "slide": "PUSH",
            "direction": "LEFT_TO_RIGHT",
            "animation": false,
            "width": "STANDARD",
            "height": "FULL"
        }
    },
    "actions": {
        "applyFocusOnMain": {
            "actionType": "JSFunction",
            "method": "applyFocusOnMain",
            "deps": "js/wcagService"
        },
        "updateArialabelForDuplicateLandmarks": {
            "actionType": "JSFunction",
            "method": "updateArialabelForDuplicateLandmarks",
            "deps": "js/wcagService"
        },
        "initializeWcag": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "applyFocusOnMain"
                },
                {
                    "action": "updateArialabelForDuplicateLandmarks"
                }
            ]
        }
    },
    "lifecycleHooks": {
        "onMount": "initializeWcag"
    },
    "props": {
        "sub-location-preference": {
            "type": "object"
        },
        "sub-panel-context": {
            "type": "object"
        }
    },
    "i18n": {
        "rightHandToolBar": [
            "UIElementsMessages"
        ]
    }
}

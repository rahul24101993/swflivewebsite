{
    "schemaVersion": "1.0.0",
    "lifecycleHooks": {
        "onMount": "initialize",
        "onUnmount": "cleanup"
    },
    "ctx": {
        "context": {
            "parser": {
                "method": "getContext",
                "deps": "js/AwBaseSublocationService"
            }
        }
    },
    "actions": {
        "initialize": {
            "actionType": "batchJob",
            "steps": [ {
                    "action": "initializePage"
                },
                {
                    "action": "updateBaseSelection"
                }
            ]
        },
        "initializePage": {
            "actionType": "JSFunctionAsync",
            "method": "initializePage",
            "inputData": {
                "provider": "{{props.provider}}",
                "baseSelection": "{{props.baseSelection}}"
            },
            "outputData": {
                "subLocationPreference": "subLocationPreference",
                "stateContext": "stateContext",
                "propertyPolicyId": "propertyPolicyId"
            },
            "deps": "js/AwBaseSublocationService"
        },
        "handleCompleteEvent": {
            "actionType": "JSFunction",
            "method": "handleCompleteEvent",
            "inputData": {
                "provider": "{{parameters.eventData}}"
            },
            "deps": "js/AwBaseSublocationService"
        },
        "cleanup": {
            "actionType": "JSFunction",
            "method": "cleanupPage",
            "inputData": {
                "stateContext": "{{data.stateContext}}",
                "propertyPolicyId": "{{data.propertyPolicyId}}"
            },
            "deps": "js/AwBaseSublocationService"
        },
        "updateBaseSelection": {
            "actionType": "JSFunction",
            "method": "updateBaseSelection",
            "inputData": {
                "selection": "{{props.baseSelection}}",
                "parentSelectionData": "{{props.subPanelContext.selectionData}}",
                "selectionQueryParamKey": "{{props.provider.selectionQueryParamKey}}"
            },
            "deps": "js/AwBaseSublocationService"
        }
    },
    "onEvent": [ {
        "eventId": "complete",
        "action": "handleCompleteEvent",
        "inputArgs": {
            "eventData": "{{eventData}}"
        }
    } ],
    "props": {
        "provider": {
            "type": "object"
        },
        "base-selection": {
            "type": "object"
        },
        "sub-panel-context": {
            "type": "object"
        }
    }
}

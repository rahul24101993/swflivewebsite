{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awChipRenderFunction",
            "deps": "js/AwChipService"
        },
        "hintPopup": {
            "actionType": "popup",
            "options": {
                "placement": "bottom-start",
                "whenParentScrolls": "close",
                "maxHeight": "200px",
                "toggleMode": true,
                "clickOutsideToClose": true,
                "forceCloseOthers": false,
                "autoFocus": false,
                "open": false
            }
        }
    },
    "data": {
        "isRefreshNeeded": true
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render"
    },
    "props": {
        "chip": {
            "type": "object"
        },
        "button-type": {
            "type": "string"
        },
        "action": {
            "type": "action"
        },
        "ui-icon-action": {
            "type": "action"
        }
    }
}

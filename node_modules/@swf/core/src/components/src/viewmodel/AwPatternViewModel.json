{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awPatternRenderFunction",
            "deps": "js/AwPatternService",
            "options": {
                "elementRefList": [
                    "input"
                ]
            }
        },
        "initialize": {
            "actionType": "JSFunctionAsync",
            "method": "initialize",
            "inputData": {
                "elementRefList": "{{parameters.elementRefList}}",
                "prop": "{{props.prop.fielddata}}"
            },
            "outputData": {
                "_states": null
            },
            "deps": "js/AwPatternService"
        },
        "dropDown": {
            "actionType": "popup",
            "inputData": {
                "options": {
                    "placement": "bottom-start",
                    "maxHeight": "210px",
                    "width": "410px",
                    "toggleMode": true
                }
            }
        }
    },
    "data": {
        "_states": null
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render",
        "onMount": "initialize"
    },
    "props": {
        "prop": {
            "type": "object"
        }
    }
}

{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awLovValRenderFunction",
            "deps": "js/AwPropertyLovValService",
            "options": {
                "elementRefList": [
                    "validEntries",
                    "dynDataprovider",
                    "selectIndex",
                    "checkLovEntries",
                    "isValidationInProgress",
                    "initialDataProviderInfo"
                ]
            }
        },
        "lovPopup": {
            "actionType": "popup",
            "options": {
                "whenParentScrolls": "close",
                "maxHeight": 300,
                "ignoreReferenceClick": true,
                "clickOutsideToClose": true,
                "forceCloseOthers": false,
                "autoFocus": false
            }
        },
        "onMount": {
            "actionType": "JSFunction",
            "method": "onMount",
            "inputData": {
                "prop": "{{props}}",
                "elementRefList": "{{parameters.elementRefList}}"
            },
            "deps": "js/AwPropertyLovValService"
        },
        "updateCheckLovEntries": {
            "actionType": "JSFunction",
            "method": "updateCheckLovEntries",
            "inputData": {
                "prop": "{{props}}",
                "elementRefList": "{{parameters.elementRefList}}"
            },
            "deps": "js/AwPropertyLovValService"
        }
    },
    "data": {
        "name": "dataProviderLOV",
        "searchBoxLOVFilterStr": {
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true"
        },
        "blankLovEntry": {
            "propInternalValue": "",
            "propDisplayValue": ""
        }
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render",
        "onMount": "onMount",
        "onUpdate": {
            "action": "updateCheckLovEntries",
            "observers": [
                "props.value"
            ]
        }
    },
    "props": {
        "prop": {
            "type": "field"
        }
    },
    "i18n": {
        "ADD_BUTTON_TITLE": [
            "BaseMessages"
        ]
    }
}

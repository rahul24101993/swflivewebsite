{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awLayoutSlotIncludeRenderFunction",
            "deps": "js/AwLayoutSlotIncludeService"
        }
    },
    "lifecycleHooks": {
        "render": "render"
    },
    "props": {
        "name": {
            "type": "string"
        },
        "context": {
            "type": "object"
        }
    },
    "ctx": {
        "activeSlot": {
            "parser": {
                "method": "getApplicableActiveSlots",
                "deps": "js/AwLayoutSlotIncludeService"
            }
        }
    }
}

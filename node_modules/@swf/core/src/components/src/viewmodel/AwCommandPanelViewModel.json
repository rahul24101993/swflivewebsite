{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awCommandPanelRenderFunction",
            "deps": "js/AwCommandPanelService"
        },
        "executeReveal": {
            "actionType": "JSFunction",
            "method": "executeReveal",
            "inputData": {
                "revealAction": "{{props.revealAction}}",
                "subPanels": "{{props.children}}"
            },
            "outputData": {
                "staticCommandSubPanels": ""
            },
            "deps": "js/AwCommandPanelService"
        },
        "navigatePanel": {
            "actionType": "batchJob",
            "steps": [ {
                    "action": "initiatePanelNav"
                },
                {
                    "action": "dispatchActiveView"
                }
            ]
        },
        "initiatePanelNav": {
            "actionType": "JSFunction",
            "method": "navigatePanel",
            "inputData": {
                "evenData": "{{parameters.eventData}}",
                "updateActiveView": "{{props.updateActiveView}}",
                "panelNavTree": "{{data.panelNavTree}}",
                "dynamicSubPanels": "{{data.dynamicSubPanels}}",
                "staticCommandSubPanels": "{{data.staticCommandSubPanels}}",
                "data": "{{data}}"
            },
            "outputData": {
                "panelNavTree": "panelNavTree",
                "dynamicSubPanels": "dynamicSubPanels",
                "currentView": "currentView"
            },
            "deps": "js/AwCommandPanelService"
        },
        "dispatchActiveView": {
            "actionType": "JSFunction",
            "method": "dispatchActiveView",
            "inputData": {
                "updateActiveView": "{{props.updateActiveView}}",
                "currentView": "{{data.currentView}}"
            },
            "deps": "js/AwCommandPanelService"
        }
    },
    "data": {
        "panelNavTree": null,
        "dynamicSubPanels": [],
        "staticCommandSubPanels": []
    },
    "props": {
        "update-hook": {
            "type": "object"
        },
        "caption": {
            "type": "string"
        },
        "hide-title": {
            "type": "object"
        },
        "anchor": {
            "type": "object"
        },
        "context": {
            "type": "object"
        },
        "submit-action": {
            "type": "action"
        },
        "sub-panel-context": {
            "type": "object"
        }
    },
    "conditions": {},
    "onEvent": [ {
        "eventId": "awPanel.navigate",
        "action": "navigatePanel",
        "inputArgs": {
            "eventData": "{{eventData}}"
        }
    } ],
    "lifecycleHooks": {
        "render": "render",
        "onMount": "executeReveal"
    }
}

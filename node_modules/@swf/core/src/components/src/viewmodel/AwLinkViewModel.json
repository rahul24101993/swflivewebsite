{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awLinkRenderFunction",
            "deps": "js/AwLinkService",
            "options": {
                "useMemo": false
            }
        },
        "linkPopup": {
            "actionType": "popup",
            "options": {
                "placement": "bottom-start",
                "toggleMode": true,
                "forceCloseOthers": false
            }
        }
    },
    "data": {
        "_dpInitialized": false
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render"
    },
    "props": {
        "prop": {
            "type": "field"
        },
        "dataprovider": {
            "type": "object"
        },
        "action": {
            "type": "action"
        },
        "link-options": {
            "type": "object"
        },
        "selectedprop": {
            "type": "object"
        },
        "command-id": {
            "type": "string"
        }
    }
}

{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awInContentSearchBoxRenderFunction",
            "deps": "js/AwInContentSearchBoxService"
        },
        "syncUpIncontextTextBox": {
            "actionType": "JSFunction",
            "method": "syncupTextbox",
            "inputData": {
                "searchState": "{{props.searchState}}",
                "inputField": "{{props}}"
            },
            "deps": "js/AwInContentSearchBoxService"
        }
    },
    "data": {
        "focused": false
    },
    "i18n": {
        "inContentSearchPlaceHolder": [ "SearchMessages" ]
    },

    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render",
        "onUpdate": [ {
            "action": "syncUpIncontextTextBox",
            "observers": [ "props.searchState.criteria.searchString" ]
        } ]
    },
    "props": {
        "prop": {
            "type": "field"
        },
        "search-state": {
            "type": "object"
        }
    }
}

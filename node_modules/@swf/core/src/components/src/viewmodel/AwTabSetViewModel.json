{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awTabSetRenderFunction",
            "deps": "js/AwTabSetService"
        },
        "updateTabSelection": {
            "actionType": "JSFunction",
            "method": "updateTabSelection",
            "inputData": {
                "tabs": "{{data}}"
            },
            "deps": "js/AwTabSetService"
        }
    },
    "data": {
        "tabsModel": [],
        "manualTabSelection": false
    },
    "props": {
        "tabs": {
            "type": "object"
        },
        "update-hook": {
            "type": "object"
        },
        "tab-set-id": {
            "type": "string"
        },
        "action": {
            "type": "action"
        },
        "sub-panel-context":{
            "type":"object"
        },
        "close-action": {
            "type": "action"
        }
    },
    "conditions": {},
    "onEvent": [ {
        "eventId": "awTab.setSelected",
        "action": "updateTabSelection",
        "cacheEventData": true
    } ],
    "lifecycleHooks": {
        "render": "render"
    }
}

{
    "schemaVersion": "1.0.0",
    "data": {
        "progressIndicatorStatus": false,
        "progressRefCount": 0,
        "subscriptions": []
    },
    "actions": {
        "registerEvents": {
            "actionType": "JSFunction",
            "method": "registerEvents",
            "inputData": {
                "name": "{{props.name}}",
                "data": "{{data}}",
                "progressIndicatorRef": "{{parameters.elementRefList}}"
            },
            "outputData": {
                "progressIndicatorRef": "progressIndicatorRef",
                "subscriptions": "subscriptions"
            },
            "deps": "js/AwProgressIndicatorService"
        },
        "render": {
            "method": "awProgressIndicatorRenderFunction",
            "options": {
                "elementRefList": [
                    "progressIndicatorRef"
                ]
            },
            "deps": "js/AwProgressIndicatorService"
        },
        "progressStart": {
            "actionType": "JSFunction",
            "method": "startProgressIndicator",
            "deps": "js/AwProgressIndicatorService"
        },
        "progressEnd": {
            "actionType": "JSFunction",
            "method": "stopProgressIndicator",
            "deps": "js/AwProgressIndicatorService"
        },
        "destroyIndicatorSubscription": {
            "actionType": "JSFunction",
            "method": "destroyIndicatorSubscription",
            "inputData": {
                "data": "{{data.subscriptions}}"
            },
            "deps": "js/AwProgressIndicatorService"
        }
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render",
        "onMount": "registerEvents",
        "onUnmount": "destroyIndicatorSubscription"
    },
    "props": {
        "name": {
            "type": "string"
        }
    },
    "i18n": {
        "LOADING_TEXT": [
            "BaseMessages"
        ]
    }
}

{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awListRenderFunction",
            "deps": "js/AwListService"
        },
        "contextPopup": {
            "actionType": "popup",
            "options": {
                "width": "200px",
                "clickOutsideToClose": true,
                "whenParentScrolls": "close"
            }
        },
        "onMount": {
            "actionType": "JSFunction",
            "method": "onMount",
            "inputData": {
                "data": "{{props}}"
            },
            "deps": "js/AwListService"
        },
        "selectionUpdated": {
            "actionType": "JSFunction",
            "method": "selectionUpdated",
            "inputData": {
                "props": "{{props}}"
            },
            "deps": "js/AwListService"
        }
    },
    "data": {
        "_init": false
    },
    "conditions": {},
    "onEvent": [],
    "ctx": {
        "mselected": null,
        "pselected": null
    },
    "lifecycleHooks": {
        "render": "render",
        "onMount": "onMount",
        "onUpdate": [ {
            "action": "selectionUpdated",
            "observers": [ "props.dataprovider.selectedObjects" ]
        } ]
    },
    "props": {
        "dataprovider": {
            "type": "object"
        },
        "show-check-box": {
            "type": "string"
        },
        "item-options": {
            "type": "object"
        },
        "show-context-menu": {
            "type": "string"
        },
        "fixed-cell-height": {
            "type": "string"
        },
        "use-virtual": {
            "type": "boolean"
        },
        "show-drop-area": {
            "type": "boolean"
        },
        "is-group-list": {
            "type": "boolean"
        },
        "has-floating-cell-commands": {
            "type": "boolean"
        },
        "show-decorators": {
            "type": "boolean"
        },
        "command-context": {
            "type": "object"
        },
        "double-click-action": {
            "type": "action"
        }
    }
}

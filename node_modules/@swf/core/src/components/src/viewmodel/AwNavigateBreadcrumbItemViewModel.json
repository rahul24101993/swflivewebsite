{
    "schemaVersion": "1.0.0",
       "actions": {
        "render": {
            "method": "awNavigateBreadcrumbItemRenderFn",
            "deps": "js/AwNavigateBreadcrumbItemService"
        },
        "chevronPopup": {
            "actionType": "popup",
            "options": {
                "whenParentScrolls": "close",
                "maxHeight": "200px",
                "toggleMode": true,
                "clickOutsideToClose": true,
                "forceCloseOthers": true
            }
        }
    },
    "props": {
        "crumb": {
            "type": "object"
        },
        "popuplist": {
            "type": "string"
        },
        "chevron-click": {
            "type": "action"
        }
    },
    "data": {},
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render"
    }
}

{
    "schemaVersion": "1.0.0",
       "actions": {
        "render": {
            "method": "awSliderRenderFunction",
            "deps": "js/AwSliderService"
        },
        "createdAction": {
            "actionType": "JSFunction",
            "method": "handleCreate",
            "deps": "js/AwSliderService",
            "inputData": {
                "props": "{{props}}"
            }
        }
    },
    "data": {
        "creationFlag":{
            "dbValue":true,
            "type":"BOOLEAN"
        }
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render",
        "onMount": "createdAction"
    },
    "i18n": {
        "DecreaseLabel": [
            "BaseMessages"
        ],
        "IncreaseLabel": [
            "BaseMessages"
        ],
        "SliderLabel": [
            "BaseMessages"
        ],
        "LeftSliderLabel": [
            "BaseMessages"
        ],
        "RightSliderLabel": [
            "BaseMessages"
        ]
    },
    "props": {
        "action-defs": {
            "type": "object"
        },
        "prop": {
            "type": "field"
        }
    }
}

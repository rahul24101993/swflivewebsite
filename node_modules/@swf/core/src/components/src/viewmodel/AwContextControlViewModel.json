{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awContextControlRenderFunction",
            "deps": "js/AwContextControlService"
        },
        "initialize": {
            "actionType": "batchJob",
            "steps": [ {
                    "action": "getContributedViews"
                },
                {
                    "action": "registerUserInfo"
                }
            ]
        },
        "getContributedViews": {
            "actionType": "JSFunctionAsync",
            "method": "fetchContributedViews",
            "inputData": {
                "context": "{{ctx}}",
                "data": "{{data}}"
            },
            "outputData": {
                "workspaceContexts": "workspaceContexts",
                "contextJson": "contextJson"
            },
            "deps": "js/AwContextControlService"
        },
        "registerUserInfo": {
            "actionType": "JSFunction",
            "method": "registerUserInfo",
            "inputData": {
                "context": "{{ctx}}"
            },
            "deps": "js/AwContextControlService"
        },
        "clearCtxData": {
            "actionType": "batchJob",
            "steps": [ {
                    "action": "unRegisterProjectDetails"
                },
                {
                    "action": "unRegisterLovDetails"
                }
            ]
        },
        "unRegisterProjectDetails": {
            "actionType": "modifyCtx",
            "inputData": {
                "methodType": "unRegister",
                "name": "isNoProject"
            }
        },
        "unRegisterLovDetails": {
            "actionType": "modifyCtx",
            "inputData": {
                "methodType": "unRegister",
                "name": "isCodeLovSet"
            }
        }
    },
    "data": {},
    "conditions": {},
    "onEvent": [],
    "ctx": {
        "workspace.workspaceId": null,
        "user": null,
        "userSession": null,
        "autoSave": null
    },
    "lifecycleHooks": {
        "onMount": "initialize",
        "render": "render",
        "onUnmount": "clearCtxData"
    },
    "props": {
        "anchor": "string"
    }
}

{
    "schemaVersion": "1.0.0",
       "actions": {
        "render": {
            "method": "awPropertyTriStateValRenderFunction",
            "deps": "js/AwPropertyTriStateValService"
        },
        "loadData": {
            "actionType": "JSFunction",
            "method": "loadTriStateInfo",
            "deps": "js/AwPropertyTriStateValService",
            "inputData": {
                "data": "{{data}}"
            },
            "outputData": {
                "triStateValues": "result.triStateValues",
                "totalFound": "result.totalFound"
            }
        }
    },
    "data": {
        "propertyRadioTrueText": "",
        "propertyRadioFalseText": ""
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render"
    },
    "dataProviders": {
        "triStateProvider": {
            "action": "loadData",
            "response": "{{data.triStateValues}}",
            "totalFound": "{{data.totalFound}}"
        }
    },
    "props":{
        "prop":{
            "type" : "field"
        }
    }
}

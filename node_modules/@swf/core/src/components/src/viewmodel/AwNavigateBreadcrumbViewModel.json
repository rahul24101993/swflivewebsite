{
    "schemaVersion": "1.0.0",
       "actions": {
        "render": {
            "method": "awNavigateBreadcrumbRenderFn",
            "deps": "js/AwNavigateBreadcrumbService",
            "options": {
                "elementRefList": [ "breadcrumb" ]
            }
        },
        "handleOverflow": {
            "actionType": "JSFunction",
            "method": "handleOverflow",
            "inputData": {
                "props": "{{props}}",
                "data": "{{data}}",
                "elementRefList": "{{parameters.elementRefList}}"
            },
            "deps": "js/AwNavigateBreadcrumbService"
        },
        "attachResizeObserver": {
            "actionType": "JSFunction",
            "method": "attachResizeObserver",
            "inputData": {
                "data": "{{data}}",
                "elementRefList": "{{parameters.elementRefList}}"
            },
            "deps": "js/AwNavigateBreadcrumbService"
        },
        "onUnMount": {
            "actionType": "JSFunction",
            "method": "onUnMount",
            "inputData": {
                "elementRefList": "{{parameters.elementRefList}}"
            },
            "deps": "js/AwNavigateBreadcrumbService"
        }
    },
    "data": {
        "rotateChevronIndex": -1,
        "overflow": {
            "index": -1,
            "crumbs": []
        },
        "resize": "true"
    },
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "render": "render",
        "onMount": "attachResizeObserver",
        "onUpdate": {
            "action": "handleOverflow",
            "observers": [ "viewModel.data.resize", "props.crumbs" ]
        },
        "onUnmount": "onUnMount"
    },
    "props": {
        "crumbs": {
            "type": "object"
        },
        "compact": {
            "type": "string"
        },
        "popuplist": {
            "type": "string"
        }
    }
}

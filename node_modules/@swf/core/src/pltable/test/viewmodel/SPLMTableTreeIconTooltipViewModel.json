{
    "schemaVersion": "1.0.0",
    "onEvent": [ {
        "eventId": "DynamicMockTableData.contentLoaded",
        "action": "exampleDataProvider"
    } ],
    "actions": {
        "loadData": {
            "actionType": "JSFunctionAsync",
            "method": "loadTreeTableData",
            "inputData": {
                "treeLoadInput": "request.treeLoadInput",
                "delayTimeTree": "{{data.delayTimeTree}}",
                "maxTreeLevel": "{{data.maxTreeLevel}}"
            },
            "outputData": {},
            "deps": "js/mockSPLMTableDataService"
        },
        "loadProperties": {
            "actionType": "JSFunctionAsync",
            "method": "loadTreeTableProperties",
            "inputData": {
                "propertyLoadInput": "request.propertyLoadInput",
                "delayTimeProperty": "{{data.delayTimeProperty}}"
            },
            "outputData": {},
            "deps": "js/mockSPLMTableDataService"
        },
        "loadColumns": {
            "actionType": "JSFunctionAsync",
            "method": "loadTreeTableColumns",
            "inputData": {
                "dataProvider": "{{data.dataProviders.exampleDataProvider}}"
            },
            "deps": "js/mockSPLMTableDataService"
        }
    },
    "data": {
        "delayTimeTree": {
            "displayName": "Tree Delay (ms)",
            "type": "INTEGER",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": "1000",
            "dispValue": "Delay (ms)",
            "labelPosition": "PROPERTY_LABEL_AT_SIDE"
        },
        "delayTimeProperty": {
            "displayName": "Property Delay (ms)",
            "type": "INTEGER",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": "2000",
            "dispValue": "Delay (ms)",
            "labelPosition": "PROPERTY_LABEL_AT_SIDE"
        },
        "maxTreeLevel": {
            "displayName": "Max Tree Level",
            "type": "INTEGER",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": "3",
            "dispValue": "Max Level",
            "labelPosition": "PROPERTY_LABEL_AT_SIDE"
        }
    },
    "dataProviders": {
        "exampleDataProvider": {
            "action": "loadData",
            "initializeAction": "loadData",
            "nextAction": "loadData",
            "previousAction": "loadData",
            "treeLoadResult": "action.result.treeLoadResult",
            "inputData": {
                "accessMode": "tree"
            }
        },
        "examplePropertyProvider": {
            "action": "loadProperties",
            "propertyLoadResult": "action.result.propertyLoadResult"
        }
    },
    "columnProviders": {
        "exampleColumnProvider": {
            "loadColumnAction": "loadColumns",
            "columns": "{{data.columns}}",
            "soaEnabled": false,
            "sortCriteria": [],
            "frozenColumnIndex": -1
        }
    },
    "grids": {
        "exampleTreeTable": {
            "dataProvider": "exampleDataProvider",
            "propertyProvider": "examplePropertyProvider",
            "columnProvider": "exampleColumnProvider",
            "addIconColumn": false,
            "gridOptions": {
                "showTreeRowHeader": false,
                "isFilteringEnabled": true,
                "enableGridMenu": true,
                "enablePinning": true,
                "enableSorting": false,
                "showTreeExpandNoChildren": false,
                "textBundle": {
                    "TwistieTooltipExpanded": "Less...",
                    "TwistieTooltipCollapsed": "More..."
                }
            }
        }
    }
}

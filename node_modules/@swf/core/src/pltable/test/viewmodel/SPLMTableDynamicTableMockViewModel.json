{
    "schemaVersion": "1.0.0",
    "onEvent": [ {
        "eventId": "splmTableDynamicMockTableData.contentLoaded",
        "action": "exampleDataProvider"
    } ],
    "actions": {
        "loadData": {
            "actionType": "JSFunctionAsync",
            "method": "loadFlatTableData",
            "inputData": {
                "data": "{{data}}"
            },
            "outputData": {
                "totalFound": "totalFound",
                "searchResults": "searchResults",
                "searchIndex": "searchIndex"
            },
            "deps": "js/mockSPLMTableDataService"
        },
        "loadColumns": {
            "actionType": "JSFunctionAsync",
            "method": "loadFlatTableColumns",
            "inputData": {
                "dataProvider": "{{data.dataProviders.exampleDataProvider}}"
            },
            "deps": "js/mockSPLMTableDataService"
        }
    },
    "data": {},
    "i18n": {},
    "dataProviders": {
        "exampleDataProvider": {
            "action": "loadData",
            "response": "{{data.searchResults}}",
            "totalFound": "{{data.totalFound}}",
            "searchIndex": "{{data.searchIndex}}",
            "json": {
                "firstPage": "SR::SAMPLE_FIRST_PAGE"
            }
        }
    },
    "columnProviders": {
        "exampleColumnProvider": {
            "loadColumnAction": "loadColumns",
            "columns": "{{data.columns}}",
            "soaEnabled": false,
            "sortCriteria": []
        }
    },
    "grids": {
        "exampleTable": {
            "dataProvider": "exampleDataProvider",
            "columnProvider": "exampleColumnProvider",
            "addIconColumn": false,
            "gridOptions": {
                "isFilteringEnabled": true,
                "enableGridMenu": true,
                "enablePinning": true,
                "enableSorting": false
            }
        }
    }
}

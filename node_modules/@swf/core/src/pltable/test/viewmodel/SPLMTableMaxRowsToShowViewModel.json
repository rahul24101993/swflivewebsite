{
  "schemaVersion":"1.0.0",
  "data":{},
  "i18n":{},
  "actions": {
      "getData": {
          "actionType": "JSFunction",
          "method": "getData",
          "deps": "js/splmTableMaxRowsToShowService",
          "outputData": {
              "totalFound": "result.totalFound",
              "searchResults": "result.searchResults"
          }
      }
  },
  "onEvent":[
    {
      "eventId":"splmTableMaxRowsToShowTest.loadTableData",
      "action":"exampleDataProvider"
    }
  ],
  "dataProviders": {
      "exampleDataProvider": {
          "action": "getData",
          "response": "{{data.searchResults}}",
          "totalFound": "{{data.totalFound}}"
      }
  },
  "columnProviders":{
    "exampleColumnProvider":{
      "columns":[
        {
          "name":"name",
          "displayName":"Name",
          "isFilteringEnabled":true,
          "isTableCommand":false,
          "typeName":"String",
          "enablePinning":true,
          "enableSorting":true,
          "enableColumnMenu":false,
          "pinnedLeft":true
        },
        {
          "name":"date",
          "displayName":"Date modified",
          "isFilteringEnabled":true,
          "isTableCommand":false,
          "typeName":"String",
          "enablePinning":true,
          "enableSorting":true,
          "enableColumnMenu":false,
          "enableColumnResizing":true,
          "pinnedLeft":false
        },
        {
          "name":"dropdownLOV",
          "displayName":"Dropdown LOV",
          "isFilteringEnabled":true,
          "isTableCommand":false,
          "typeName":"STRING",
          "enablePinning":true,
          "enableSorting":true,
          "enableColumnMenu":false,
          "enableColumnResizing":true,
          "pinnedLeft":false,
          "propertyName":"aw2_ExternalReference_LOV"
        },
        {
          "name":"aw2_TypedReference",
          "displayName":"Typed Reference",
          "isFilteringEnabled":true,
          "isTableCommand":false,
          "typeName":"OBJECT",
          "enablePinning":false,
          "enableSorting":true,
          "enableColumnMenu":false,
          "pinnedLeft":false
        }
      ]
    }
  },
  "grids":{
    "exampleTable":{
      "dataProvider":"exampleDataProvider",
      "columnProvider":"exampleColumnProvider",
      "addIconColumn":false,
      "gridOptions":{
        "enableGridMenu":true,
        "enablePinning":true,
        "enableSorting":false,
        "maxRowsToShow":4
      }
    }
  }
}

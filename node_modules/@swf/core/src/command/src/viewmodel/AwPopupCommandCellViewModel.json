{
    "schemaVersion": "1.0.0",
    "actions": {
        "render": {
            "method": "awCommandRenderFunction",
            "options": {
                "elementRefList": [
                    "cmdState"
                ]
            },
            "deps": "js/AwPopupCommandCellService"
        },
        "openCommandPopup": {
            "actionType": "popup",
            "options": {
                "autoFocus": true,
                "closeParentPopup": true,
                "clickOutsideToClose": true,
                "flipBehavior": "opposite",
                "forceCloseOthers": false,
                "placement": "right-start",
                "toggleMode": true,
                "whenParentScrolls": "follow"
            }
        }
    },
    "ctx": {
        "commandClass": {
            "parser": {
                "method": "getCommandClassMemo",
                "deps": "js/AwPopupCommandCellService"
            }
        }
    },
    "lifecycleHooks": {
        "render": "render"
    }
}

{
    "schemaVersion": "1.0.0",
    "data": {
        "_swfCommandComponent": true
    },
    "actions": {
        "render": {
            "method": "awCommandRenderFunction",
            "options": {
                "elementRefList": [
                    "cmdState"
                ]
            },
            "deps": "js/AwCommandService"
        },
        "openCommandPopup": {
            "actionType": "popup",
            "options": {
                "advancePositioning": true,
                "autoFocus": true,
                "clickOutsideToClose": true,
                "closeWhenEsc": true,
                "flipBehavior": "opposite",
                "selectedElementCSS": ".aw-state-selected",
                "toggleMode": true,
                "whenParentScrolls": "follow",
                "forceCloseOthers": false
            }
        }
    },
    "ctx": {
        "commandClass": {
            "parser": {
                "method": "getCommandClassMemo",
                "deps": "js/AwCommandService"
            }
        }
    },
    "lifecycleHooks": {
        "render": "render"
    }
}

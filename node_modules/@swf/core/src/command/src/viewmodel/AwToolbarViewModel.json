{
    "schemaVersion": "1.0.0",
    "actions": {
        "initialize": {
            "actionType": "JSFunctionAsync",
            "deps": "js/AwToolbarService",
            "method": "awToolbarInitFunction",
            "inputData": {
                "0": {
                    "firstAnchor": "{{props.firstAnchor}}",
                    "secondAnchor": "{{props.secondAnchor}}"
                }
            },
            "outputData": {
                "firstCommandList": "firstCommandList",
                "secondCommandList": "secondCommandList",
                "trace": "trace"
            }
        },
        "refreshServerVisibility": {
            "actionType": "JSFunctionAsync",
            "deps": "js/AwToolbarService",
            "method": "refreshServerVisibilityToolbar",
            "inputData": {
                "0": {
                    "firstAnchor": "{{props.firstAnchor}}",
                    "secondAnchor": "{{props.secondAnchor}}",
                    "firstCommandList": "{{data.firstCommandList}}",
                    "secondCommandList": "{{data.secondCommandList}}",
                    "visibilityLoader": "{{props.visibilityLoader}}",
                    "trace": "{{data.trace}}"
                }
            },
            "outputData": {
                "visibleServerCommands": ""
            }
        },
        "render": {
            "method": "awToolbarRenderFunction",
            "options": {},
            "deps": "js/AwToolbarService"
        }
    },
    "data": {},
    "conditions": {},
    "onEvent": [],
    "lifecycleHooks": {
        "onMount": "initialize",
        "onUpdate": {
            "action": "refreshServerVisibility",
            "observers": [
                "props.visibilityLoader",
                "viewModel.data.firstCommandList",
                "viewModel.data.secondCommandList"
            ]
        },
        "render": "render"
    },
    "props": {
        "first-anchor": {
            "type": "string"
        },
        "second-anchor": {
            "type": "string"
        },
        "orientation": {
            "type": "string"
        },
        "reverse": {
            "type": "object"
        },
        "reverse-second": {
            "type": "reverse"
        },
        "show-command-labels": {
            "type": "object"
        },
        "visibility-loader": {
            "type": "object"
        },
        "context": {
            "type": "object"
        },
        "overflow": {
            "type": "string"
        }

    }
}

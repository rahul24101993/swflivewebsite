    "antiPatterns": {
        "(\\\\|/)[\\s\\S]*(\\\\|/)js(\\\\|/)[\\s\\S]*\\.js$": {
            "define\\s*\\(\\s*\\[\\s*[\\s\\S]*\\]\\s*,[\\s\\S]*(function)\\s*\\([\\s\\S]*\\);": {
                "message": "Don't use requirejs define to author js file. Use ES6 instead",
                "exceptions": [
                    "node_modules/aw-hosting-browser-inter-op/src/javascript/tests/testfixture/js/logger.js",
                    "node_modules/aw-hosting-browser-inter-op/src/javascript/tests/testfixture/js/handlers/loggerForwardService.js",
                    "node_modules/aw-hosting-browser-inter-op/src/javascript/tests/testfixture/js/handlers/selectionProviderSvc_2014_02.js"
                ]
            },
            "app\\.(factory|service)\\([\\s]*'[\\S]*',[\\s]*\\[[\\s]*": "Do not inject services in angular way, Use ES6 import instead"
        },
        "\\Test.js$": {
            "AwRootAppModule": "Don't initialize the AwRootAppModule in your test. This is done centrally in karma-bootstrap.js."
        },
        "\\.(css|scss)$": {
            "absolute ": "Avoid using absolute positioning."
        },
        "\\.(java|css|scss|xml|js)$": {
            "\\t": "Remove TABs!"
        },
        "\\.(svg)$": {
            "<\\?xml": "Remove <?xml element!",
            "<!DOCTYPE": "Remove <!DOCTYPE element!",
            "<title>": "Remove <title> element!"
        }
    },
    "localeServiceI18n": {
        "\\.js$": {
            "(\\.getTextPromise\\( |\\.getLoadedText\\( )": "Don't use getTextPromise and getLoadedText"
        }

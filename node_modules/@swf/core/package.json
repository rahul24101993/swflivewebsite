    "@swf/eslint-config-swf": "next",
    "@swf/eslint-plugin-swf": "next"
  },
  "scripts": {
    "clean": "rimraf out *.log *.tgz performance.json coverage node_modules/.cache",
    "test": "node node_modules/@swf/tooling/js/jest_wrapper.js test --watchAll=false",
    "test:debug": "react-app-rewired test",
    "test:debugChrome": "react-app-rewired --inspect-brk test --runInBand --no-cache",
    "test:coverage": "node node_modules/@swf/tooling/js/jest_wrapper.js test --watchAll=false --coverage",
    "posttest:coverage": "gulp --gulpfile node_modules/@swf/tooling/js/codeCoverageProcessor.js testReport --cwd .",
    "pregulp": "node node_modules/@swf/tooling/js/createCache.js",
    "gulp": "gulp -f node_modules/@swf/tooling/gulpfile.js --cwd .",
    "eslint": "eslint src test --ext .js,.json",
    "beautify": "js-beautify src/**/*.js",
    "audit": "node node_modules/@swf/tooling/js/audit.js",
    "html-lint": "node node_modules/@swf/tooling/js/audit/html_i18n.js"
  },
  "config-overrides-path": "config-overrides.js",
  "setupTestFrameworkScriptFile": "src/setupTests.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "Chrome >= 61",
      "Firefox >= 60",
      "Safari >= 11.1",
      "Opera >= 48",
      "Samsung >= 8.2",
      "Android >= 67",
      "ChromeAndroid >= 74",
      "FirefoxAndroid >= 66",
      "Edge >= 79"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "not IE <= 11"
    ]
  },
  "publishConfig": {
    "registry": "https://artifacts.industrysoftware.automation.siemens.com/artifactory/api/npm/npm/",
